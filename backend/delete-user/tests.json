{
  "tests": [
    {
      "name": "Handle OPTIONS request",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200,
      "expectedBody": "",
      "bodyMatcher": "exact"
    },
    {
      "name": "Delete user without admin token returns 403",
      "method": "DELETE",
      "path": "/",
      "body": {
        "user_id": "123"
      },
      "expectedStatus": 403
    },
    {
      "name": "Delete user without user_id returns 400",
      "method": "DELETE",
      "path": "/",
      "headers": {
        "X-Admin-Token": "test-admin-token"
      },
      "body": {
        "user_id": ""
      },
      "expectedStatus": 400
    }
  ]
}